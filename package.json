{
  "name": "@aneoconsultingfr/order-github-release-notes",
  "version": "0.0.1",
  "description": "Organize auto generated changelog using the GitHub API",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "start": "nr dev && node cli.mjs",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneoconsulting/order-github-release-notes.git"
  },
  "keywords": [
    "github",
    "release",
    "changelog",
    "notes"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./cli.mjs",
  "files": [
    "dist",
    "*.mjs"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Est√©ban Soubiran <esoubiran@aneo.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneoconsulting/order-github-release-notes/issues"
  },
  "homepage": "https://github.com/aneoconsulting/order-github-release-notes#readme",
  "dependencies": {
    "changelogithub": "^0.12.9",
    "consola": "^3.1.0",
    "cac": "^6.7.14",
    "ofetch": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/node": "^18.16.3",
    "eslint": "^8.39.0",
    "jiti": "^1.18.2",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  },
  "packageManager": "pnpm@8.3.1"
}
